# Variables d'environnement pour ChronoTime
# Copiez ce fichier vers .env et remplissez avec vos vraies valeurs

# MongoDB Connection String
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/chronomontagne?retryWrites=true&w=majority

# JWT Secret Key
JWT_SECRET=your_jwt_secret_key_here

# Server Port
PORT=9000

# Environment
NODE_ENV=development
